<header class="header_top">
	{{#with data}}
	<div class="header_brandName">{{brandName}}</div>

	<div class="header_links_box">
		{{#each links}}
		<a href="{{url}}" class="header_links">{{text}}</a>
		{{/each}}
	</div>

	{{/with}}
</header>

<div class="header_bottom">
	<div class="header_bottom_box">Ad</div>
	{{#with data}}

	<a href="{{ad.url}}" class="header_text">{{ad.text}}</a>

	<div class="header_userOnline">Users online: {{userOnline}}</div>
	{{/with}}
</div>